{
  "name": "vue-vite-starter",
  "private": false,
  "version": "0.0.2",
  "description": "基于vue3、typesScript、vite3 开发脚手架",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/funnyzak/vue-vite-starter.git"
  },
  "keywords": [
    "vue",
    "vue3",
    "vite",
    "typescript"
  ],
  "author": "leon <silenceace@gmail.com>",
  "scripts": {
    "i": "pnpm install",
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "serve:test": "vite preview --mode test",
    "npm:check": "npx npm-check-updates",
    "clean": "npx rimraf node_modules",
    "clean:cache": "npx rimraf node_modules/.cache",
    "p": "plop"
  },
  "dependencies": {
    "vue": "^3.2.37"
  },
  "devDependencies": {
    "@types/node": "^18.0.6",
    "@vitejs/plugin-legacy": "^2.0.0",
    "@vitejs/plugin-vue": "^3.0.1",
    "less": "^4.1.3",
    "plop": "^3.1.1",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "sass": "^1.53.0",
    "stylus": "^0.58.1",
    "typescript": "^4.7.4",
    "vite": "^3.0.2",
    "vue-tsc": "^0.38.9"
  },
  "engines": {
    "node": ">= 14.18.0"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/funnyzak/vue-vite-starter/issues"
  },
  "homepage": "https://github.com/funnyzak/vue-vite-starter#readme",
}
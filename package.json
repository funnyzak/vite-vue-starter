{
  "name": "vite-vue-starter",
  "private": false,
  "version": "0.1.0",
  "description": "基于vue3、typesScript、vite3 开发脚手架",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/funnyzak/vite-vue-starter.git"
  },
  "keywords": [
    "vue",
    "vue3",
    "vite",
    "typescript"
  ],
  "author": "leon <silenceace@gmail.com>",
  "scripts": {
    "i": "pnpm install",
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "serve:test": "vite preview --mode test",
    "npm:check": "npx npm-check-updates",
    "clean": "npx rimraf node_modules",
    "clean:cache": "npx rimraf node_modules/.cache",
    "lint:eslint": "eslint --fix --ext .js,.ts,.vue ./src",
    "lint:format": "prettier --write --loglevel warn \"src/**/*.{js,ts,json,tsx,css,less,vue,html,md}\"",
    "lint:lint-staged": "lint-staged -c ",
    "lint:pretty": "pretty-quick --staged",
    "p": "plop",
    "prepare": "husky install"
  },
  "dependencies": {
    "jsencrypt": "^3.2.1",
    "pinia": "^2.0.17",
    "pinia-plugin-persist": "^1.0.0",
    "vue": "^3.2.37",
    "vue-i18n": "^9.1.10",
    "vue-types": "^4.2.0",
    "web-storage-cache": "^1.1.1"
  },
  "devDependencies": {
    "@intlify/vite-plugin-vue-i18n": "^5.0.1",
    "@types/node": "^18.6.1",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "@vitejs/plugin-legacy": "^2.0.0",
    "@vitejs/plugin-vue": "^3.0.1",
    "eslint": "^8.20.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-define-config": "^1.5.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.3.0",
    "husky": "^8.0.1",
    "less": "^4.1.3",
    "lint-staged": "^13.0.3",
    "plop": "^3.1.1",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "sass": "^1.54.0",
    "stylus": "^0.58.1",
    "typescript": "^4.7.4",
    "vite": "^3.0.3",
    "vite-plugin-eslint": "^1.7.0",
    "vue-tsc": "^0.39.0"
  },
  "lint-staged": {
    "*.{js,cjs,jsx,vue,ts,tsx}": [
      "prettier --write",
      "eslint --fix --cache"
    ],
    "*.{md,html,json,css,less,scss,sass}": [
      "prettier --write"
    ]
  },
  "engines": {
    "node": ">= 14.18.0"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/funnyzak/vite-vue-starter/issues"
  },
  "homepage": "https://github.com/funnyzak/vite-vue-starter#readme"
}
